<!-- -->
<launch>
	<arg name="width" 						default="400" />
	<arg name="height" 						default="550" />
	<arg name="info_ratio" 					default="0.5" />	
	<node pkg="testing_ui" type="testing_ui" name="testing_ui" output="screen">
		<param name="width" 						value="$(arg width)" />
		<param name="height" 						value="$(arg height)" />		
		<param name="info_ratio" 					value="$(arg info_ratio)" />		
	</node>

</launch>
